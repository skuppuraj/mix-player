{
  "name": "mix-player",
  "productName": "Mix Player view",
  "version": "1.0.0",
  "description": "One music player to control multiple streaming platform",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . mix-player --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Mix Player'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skuppuraj/mix-player.git"
  },
  "keywords": [
    "Music",
    "player"
  ],
  "author": "Kuppuraj",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skuppuraj/mix-player/issues"
  },
  "homepage": "https://github.com/skuppuraj/mix-player#readme",
  "devDependencies": {
    "electron": "^4.0.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.0.1",
    "electron-winstaller": "^4.0.0",
    "install": "^0.12.2"
  }
}
